<?xml version="1.0" encoding="utf-8"?>


<TableRow xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <TextView
        android:id="@+id/team_member_index"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_column="1"
        android:text="TextView" />

    <EditText
        android:id="@+id/team_member_id"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:layout_marginHorizontal="20dp"
        android:background="@drawable/rulebook_btn_bg"
        android:cursorVisible="true"
        android:focusable="true"
        android:fontFamily="@font/inter_bold"
        android:importantForAutofill="no"
        android:inputType="text"
        android:maxLines="1"
        android:paddingHorizontal="10dp"
        android:textColor="@color/black"
        android:textColorHint="#96A4AF"
        android:textSize="18sp"
        android:layout_marginTop="5dp"
        android:layout_column="2"
        tools:ignore="SpeakableTextPresentCheck,DuplicateClickableBoundsCheck" />

    <ImageView
        android:id="@+id/delete_team_member"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_column="3"
        app:srcCompat="@drawable/minus" />
</TableRow>
